{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "pages/*": ["src/pages/*"],
      "components/*": ["src/components/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"],
      "hooks/*": ["src/hooks/*"],
      "apis/*": ["src/apis/*"]
    }
  },
  "include": ["src"]
}
